# ----------------
# Library
# ----------------

#
# felica_cc_stub_rcs956
#

FELICA_CC_STUB_RCS956_LIB= \
	felica_cc_stub_rcs956
FELICA_CC_STUB_RCS956_SRC_DIRS= \
	$(COMMON_DIR)/felica/command/stub/rcs956
FELICA_CC_STUB_RCS956_SRCS= \
	felica_cc_stub_rcs956.c

FELICA_CC_STUB_RCS956_LIB_A= \
	lib$(FELICA_CC_STUB_RCS956_LIB).a
FELICA_CC_STUB_RCS956_LIB_SO= \
	lib$(FELICA_CC_STUB_RCS956_LIB).so
FELICA_CC_STUB_RCS956_LIBS= \
	$(FELICA_CC_STUB_RCS956_LIB_A) \
	$(FELICA_CC_STUB_RCS956_LIB_SO)
FELICA_CC_STUB_RCS956_OBJS= \
	$(FELICA_CC_STUB_RCS956_SRCS:.c=.o)
FELICA_CC_STUB_RCS956_SOBJS= \
	$(FELICA_CC_STUB_RCS956_SRCS:.c=.So)

SRC_DIRS+=	$(FELICA_CC_STUB_RCS956_SRC_DIRS)
LIB_TARGETS+=	$(FELICA_CC_STUB_RCS956_LIBS)
EXE_LDADD+=	-l$(FELICA_CC_STUB_RCS956_LIB)

# ----------------
# Rule
# ----------------

$(FELICA_CC_STUB_RCS956_LIB_A)::	$(FELICA_CC_STUB_RCS956_OBJS)
	$(AR) r $@ \
		$(FELICA_CC_STUB_RCS956_OBJS)

$(FELICA_CC_STUB_RCS956_LIB_SO)::	$(FELICA_CC_STUB_RCS956_SOBJS)
	$(CC) -shared -Wl,-x -o $@ \
		$(LDFLAGS) \
		$(FELICA_CC_STUB_RCS956_SOBJS) \
		$(FELICA_CC_STUB_RCS956_LDADD)
